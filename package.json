{
  "name": "postcards",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "convex": "npx convex dev",
    "cdash": " npx convex dashboard",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "npx expo start -c --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "test": "jest --watchAll",
    "generate-themes": "tamagui generate-themes ./themes/theme-builder.ts ./themes/themes.ts",
    "clean": "rm -rf android ios node_modules bun.lockb yarn.lock package-lock.json",
    "clear": "rm -rf android ios node_modules bun.lockb yarn.lock package-lock.json && yarn install",
    "rerun": "rm -rf android ios node_modules bun.lockb yarn.lock package-lock.json && yarn install && yarn android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@tamagui/config": "^1.108.4",
    "@tamagui/linear-gradient": "^1.108.4",
    "@tamagui/lucide-icons": "^1.108.4",
    "@tamagui/toast": "^1.108.4",
    "babel-preset-expo": "^11.0.6",
    "burnt": "^0.12.2",
    "convex": "^1.14.1",
    "expo": "^51.0.28",
    "expo-blur": "~13.0.2",
    "expo-font": "~12.0.6",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.15",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "^1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "openai": "^4.56.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.108.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.18.4",
    "@expo/metro-runtime": "~3.2.1",
    "@tamagui/babel-plugin": "^1.108.4",
    "@tamagui/metro-plugin": "^1.108.4",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
